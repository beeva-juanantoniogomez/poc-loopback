{
  "name": "Movie",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "duration": {
      "type": "number"
    },
    "poster": {
      "type": "string"
    },
    "year": {
      "type": "number"
    },
    "country": {
      "type": "string"
    },
    "sinopsis": {
      "type": "string"
    },
    "rating_filmaffinity": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "directors": {
      "type": "hasAndBelongsToMany",
      "model": "Artist",
      "foreignKey": "movieId",
      "through": "ArtistMovie",
      "options": {
        "nestRemoting": true
      }
    },
    "actors": {
      "type": "hasAndBelongsToMany",
      "model": "Artist",
      "foreignKey": "movieId",
      "through": "ArtistMovie",
      "options": {
        "nestRemoting": true
      }
    },
    "publisher": {
      "type": "hasOne",
      "model": "User",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "country": {
      "type": "hasOne",
      "model": "Country",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
