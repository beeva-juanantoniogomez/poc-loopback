{
  "name": "Artist",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "fullname": {
      "type": "string",
      "required": true
    },
    "birthdate": {
      "type": "date"
    },
    "photo": {
      "type": "string"
    },
    "gender": {
      "type": "string",
      "required": true,
      "enum": [
        "male",
        "female"
      ]
    }
  },
  "validations": [],
  "relations": {
    "movies": {
      "type": "hasAndBelongsToMany",
      "model": "Movie",
      "foreignKey": "artistId",
      "through": "MovieArtist",
      "options": {
        "nestRemoting": true
      }
    },
    "country": {
      "type": "hasOne",
      "model": "Country",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
